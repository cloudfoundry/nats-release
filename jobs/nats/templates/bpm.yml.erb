---
processes:
- name: nats
  limits:
    open_files: 100000
  executable: /var/vcap/packages/nats-server/bin/nats-server
  args:
  - -c
  - "/var/vcap/jobs/nats/config/nats.conf"
- name: nats-v2-migrate
  executable: /var/vcap/packages/nats-v2-migrate/bin/server
  args:
  - -c
  - "/var/vcap/jobs/nats/config/migrator-config.json"
  unsafe:
    - privileged: true
